@model List<nerdy.Models.Snack>
<div class="shelf shelf_1">
    <table class="table">
        <thead>
            <tr>
                <th scope="col">Snack Name</th>
                <th scope="col">Purchase Location</th>
            </tr>
        </thead>
        <tbody>
            @foreach(var snack in Model){
                <tr>
                    <td>@snack.Name</td>
                    <td>@snack.PurchaseLocations</td>
                </tr>
            }
        </tbody>
    </table>
